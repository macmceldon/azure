{
	"name": "CopyItemMstrFromCsvToDb",
	"properties": {
		"activities": [
			{
				"name": "SourceToSink",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"ITEM_NBR": "ITEM_NBR",
							"MDS_FAM_ID": "MDS_FAM_ID",
							"ITEM1_DESC": "ITEM1_DESC",
							"UPC_NBR": "UPC_NBR",
							"BASE_DIV_NBR": "BASE_DIV_NBR",
							"COUNTRY_CODE": "COUNTRY_CODE",
							"COUNTRY_NAME": "COUNTRY_NAME",
							"DEPT_NBR": "DEPT_NBR"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "HviItemMasterCsv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "HviPocDbSink",
						"type": "DatasetReference"
					}
				]
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}